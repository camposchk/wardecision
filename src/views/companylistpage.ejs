<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ListPage</title>

    <link rel="icon" type="image" href="assets/logo.ico">

    <!-- Styles -->
    <link rel="stylesheet" href="css/companylist.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Inria+Sans:ital,wght@0,300;0,400;0,700;1,300;1,400;1,700&display=swap"
        rel="stylesheet">

</head>

<body>
    <header>
        <navbar-component></navbar-component>
    </header>

    <main>
        <section class="section1">
            <div class="search-area">
                <form class="form-inline">
                    <input id="Search" onkeyup="searchFunction()" class="search" type="search"
                    placeholder="Pesquisar Filiais" aria-label="Search">
                    <button class="btn-search" type="button"><i class="bi bi-search"></i></button>
                </form>
            </div>
        </section>

        <section class="section2">
            <div class="card">
                <div class="card-body">
                    <h4>Nome: </h4>
                    <h6>País: </h6>
                    <h6>Estado: </h6>
                    <div class="card-btn-area">
                        <button>Editar</button>
                        <button>Deletar</button>
                    </div>
                </div>
            </div>
        </section>

        <!-- Modal exclusão de empresa  -->
        <div id="ModalDelUser" class="modal fade">
            <div class="modal-dialog border-rounded">
                <div class="modal-content strongbg">
                    <div class="modal-header border-0">
                        <button type="button" class="btn btn-close" style="background-color:#b9b9b9;"
                            data-bs-dismiss="modal"></button>
                    </div>
                    <div class="modal-body">
                        <h3 class="text-center mx-auto">Remover uma Filial</h3>
                        <form>
                            <div class="col-6 mt-5 mx-auto desc-modal">
                                <label for="senha" class="form-label text-center remove-label">
                                    <input type="text" class="nome-hardware mb-2" name="user_name"
                                        value="Nome: {{user.name}}" readonly>
                                    <input type="text" class="nome-hardware mb-2" name="user_nickname"
                                        value="Usuário: {{user.nickname}}" readonly>
                                    <input type="email" class="nome-hardware mb-2" name="user_email"
                                        value="Email: {{ user.email }}" readonly>
                                    <input type="text" class="nome-hardware mb-2" name="user_cpf"
                                        value="CPF: {{ user.cpf }}" readonly>
                                    <input type="date" class="nome-hardware mb-2" name="user_birth_date"
                                        value="{{ user.birth_date }}" readonly>
                                </label>
                            </div>
                            <div class="col-6 my-5 mx-auto button-modal-confirm">
                                <button class="btn btn-danger mx-auto btn-lg">
                                    <input type="submit">Confirmar</input>
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>

        <!-- Modal edição de empresa  -->
        <div id="ModalEditUser" class="modal fade">
            <div class="modal-dialog border-rounded">
                <div class="modal-content strongbg">
                    <div class="modal-header border-0">
                        <button type="button" class="btn btn-close" style="background-color:#b9b9b9;"
                            data-bs-dismiss="modal"></button>
                    </div>
                    <div class="modal-body">
                        <h3 class="text-center mx-auto">Editar uma Filial</h3>
                        <form>
                            <div class="col-6 mt-5 mx-auto desc-modal">
                            </div>
                            <div class="col-6 my-5 mx-auto button-modal-confirm">
                                <button class="btn btn-danger mx-auto btn-lg">
                                    <input type="submit">Confirmar</input>
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>

    </main>

    <footer class="site-footer">
        <footer-component></footer-component>
    </footer>

    <!-- Scripts -->
    <script src="js/navbar_component.js"></script>
    <script src="js/footer_component.js"></script>
    <script src="js/searchbarjs"></script>
    <script defer src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
</body>

</html>